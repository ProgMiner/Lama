var x, y, z;

fun zip (x) {
  case x of Pair (x, y) ->
    case x of
      Nil          ->  Nil
    | Cons (x, xs) -> case y of
                         Nil ->  Nil
  		       | Cons (y, ys) ->  Cons (Pair (x, y), zip (Pair (xs, ys)))
		       esac
    esac
  esac
}

fun unzip (x) {
  case x of
    Nil                    ->  Pair (Nil, Nil)
  | Cons (Pair (x, y), tl) ->
       case unzip (tl) of
         Pair (xs, ys) ->  Pair (Cons (x, xs), Cons (y, ys))
       esac
  esac
}

fun printList (l) {
  case l of
    Nil -> skip
  | Cons (x, xs) -> write (x); printList (xs)
  esac
}

